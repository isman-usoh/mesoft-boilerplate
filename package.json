{
  "name": "meschool",
  "version": "1.0.0",
  "description": "",
  "main": "bin/server",
  "scripts": {
    "start": "cross-env NODE_ENV=production node ./bin/server",
    "deploy": "npm version patch -m \"Release v%s\"",
    "postversion": "git push origin master && git push --tags",
    "postinstall": "npm run dll",
    "build": "npm run build-client && npm run build-server",
    "build-client": "cross-env NODE_ENV=production webpack -p --config ./webpack/webpack.config.client.prod.babel.js --display-error-details",
    "build-server": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.config.server.prod.babel.js --display-error-details",
    "build-api": "tsoa routes -c tsoa-prod.json && tsoa swagger -c tsoa-prod.json && bin/swagger.sh",
    "build-api:win": "tsoa routes -c tsoa-prod.json && tsoa swagger -c tsoa-prod.json && bin/swagger.bat",
    "client": "nodemon ./webpack/webpack-dev-server --config nodemon-client.json",
    "server": "concurrently --kill-others \"npm run server-wacth\" \"npm run server-runner\"",
    "server:win": "start npm run server-wacth && start npm run server-runner",
    "server-runner": "nodemon --delay 2500ms ./bin/server --watch ./build/server",
    "server-wacth": "webpack --config ./webpack/webpack.config.server.dev.babel.js --display-error-details --watch ",
    "lint": "tslint --exclude node_modules src/**/*.ts src/**/*.tsx",
    "dll": "webpack --config ./webpack/webpack.dll.config.babel.js --display-error-details",
    "dev:win": "start npm run client && start npm run server-wacth && start npm run server-runner",
    "dev:linux": "npm run client | npm run server-wacth | npm run server-runner",
    "clean": "rimraf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://isman-usoh@bitbucket.org/mehubdeveloper/meschool.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "homepage": "https://bitbucket.org/mehubdeveloper/meschool#readme",
