    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.18.1",
    "classnames": "^2.2.5",
    "compression": "^1.7.0",
    "cors": "^2.8.4",
    "cross-env": "^5.0.5",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "helmet": "^3.8.1",
    "intl": "^1.2.5",
    "intl-locales-supported": "^1.0.0",
    "isomorphic-fetch": "^2.2.1",
    "method-override": "^2.3.9",
